{
  "name": "samffy/localstack-poc",
  "type": "project",
  "description": "This project is a proof of concept to test localstack usage.",
  "keywords": [
    "php", "localstack", "aws", "aws-sdk", "aws-sdk-php", "symfony"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Samuel Jobard",
      "email": "samuel.jobard@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "aws/aws-sdk-php": "~3.133.0",
    "symfony/console": "~5.0",
    "symfony/dotenv": "~5.0"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
